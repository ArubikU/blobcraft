  "keywords": ["minecraft", "blob-storage", "http-api", "file-upload", "typescript"],
  "author": "Arubik",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  },
  "files": ["dist/**/*", "README.md"],
  "repository": {
    "type": "git",
